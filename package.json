{
  "name": "squarefundr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "dev:clean": "lsof -ti:3000,3001,3002,3003,3004,3005,3006,3007,3008,3009,3010 | xargs kill -9 2>/dev/null || true && next dev",
    "kill-port": "lsof -ti:3000,3001,3002,3003,3004,3005,3006,3007,3008,3009,3010 | xargs kill -9 2>/dev/null || true",
    "kill-next": "pkill -f 'next-server' 2>/dev/null || pkill -f 'next dev' 2>/dev/null || pkill -f '.next' 2>/dev/null || true",
    "restart": "npm run kill-port && npm run kill-next && rm -rf .next && next dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@paypal/paypal-js": "^8.2.0",
    "@paypal/react-paypal-js": "^8.8.3",
    "@stripe/stripe-js": "^7.3.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.10",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.57.0",
    "stripe": "^18.2.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.4",
    "supabase": "^2.24.3",
    "tailwindcss": "^3.4.17",
    "tempo-devtools": "^2.0.106",
    "typescript": "^5"
  }
}
